                </span>
                <a href="/" class="admin admin-text">
                    @ViewBag.role
                  </a>
                <img src="~/image/14.png" class="admin admin-1">
            </span>
        </div>
        <div id="page">
            <div class="container-fluid">
                <div id="wrapper" class="wrapper">
                </div>
                <button id="mm-menu-toggle" class="mm-menu-toggle"></button>
                <nav id="mm-menu" class="mm-menu">
                    <div class="mm-menu__header">
                    </div>
                    <ul class="mm-menu__items">
                        <li class="mm-menu__item">
                            <a class="mm-menu__link" onclick="navpage('/Home/Confirm','督导','主页')">
                                <img src="~/image/1.png">
                                <div class="mm-menu__link-text">
                                    主页
                                </div>
                            </a>
                        </li>
                        <li class="mm-menu__item">
                            <a class="mm-menu__link" onclick="navpage('/Schedule/Schedule','督导','进度表')">

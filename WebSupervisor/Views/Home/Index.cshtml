                                <div class="mm-menu__link-text">
                                    教师信息
                                </div>
                            </a>
                        </li>
                        <li class="mm-menu__item">
                            <a class="mm-menu__link" onclick="navpage('/Supervisor/Supervisor','督导','督导信息')">
                                <img src="~/image/6.png">
                                <div class="mm-menu__link-text">
                                    督导信息
                                </div>
                            </a>
                        </li>
                        <li class="mm-menu__item">
                            <a class="mm-menu__link" onclick="navpage('/Home/Set','督导','设置')">
                                <img src="image/8.png">
                                <div class="mm-menu__link-text">
                                    设置
                                </div>
                            </a>
                        </li>
                        <li class="mm-menu__item">
                            @{ if (ViewBag.role == "超级管理员")
                                {
                                    <a class="mm-menu__link" onclick="navpage('/Power/key','督导','权限管理')">
                                        <img src="image/7.png">
                                        <div class="mm-menu__link-text">
                                            权限管理
                                        </div>
                                    </a>
                                }
                            }
                            </li>

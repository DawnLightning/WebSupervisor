        <script src="~/js/jquery.min.js">
        </script>
        <script src="~/js/bootstrap.min.js">
        </script>
        <script src="~/js/tabs.js">
        </script>
        <script src="~/js/materialMenu.js">
        </script>
        <script src="~/js/navpage.js">
        </script>
        <script>
          var act=window.location.hash;
        if(act.substring(0,3)=="#!/")
        {
          showpage(act.substring(3));
        }else{
          showpage("/Home/Confirm");
        }
        var menu = new Menu;
        </script>
